{
    "plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss"],
    "overrides": [
        {
            "files": "*.astro",
            "options": {
                "parser": "astro"
            }
        }
    ],
    "tabWidth": 4,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 100,
    "bracketSpacing": true
}
